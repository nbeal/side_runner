{
  "name": "selenium-side-runner",
  "version": "4.0.0",
  "description": "Run Selenium IDE projects in cli",
  "repository": "https://github.com/SeleniumHQ/selenium-ide",
  "scripts": {
    "build": "babel -d dist src --ignore=src/__test__",
    "prepublishOnly": "yarn build",
    "watch": "yarn build -w"
  },
  "bin": {
    "selenium-side-runner": "dist/index.js"
  },
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "selenium",
    "command",
    "line"
  ],
  "license": "Apache-2.0",
  "private": false,
  "dependencies": {
    "commander": "^8.2.0",
    "glob": "^7.2.0",
    "global-npm": "^0.4.1",
    "jest-cli": "27.3.0",
    "jest-environment-selenium": "2.1.2",
    "js-beautify": "^1.14.0",
    "js-yaml": "^4.1.0",
    "rimraf": "^3.0.2",
    "selenium-webdriver": "4.0.0",
    "selianize": "^3.17.0",
    "semver": "^7.3.5",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/preset-env": "^7.15.8"
  }
}
